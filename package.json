{
	"name": "svelte-gql",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev:only": "vite dev",
		"build:only": "vite build",
		"tailwind:watch": "cross-env TAILWIND_MODE=watch cross-env NODE_ENV=development postcss src/styles/tailwind.css -o src/styles/tailwind-output.css -w",
		"tailwind:build": "cross-env TAILWIND_MODE=build cross-env NODE_ENV=production postcss src/styles/tailwind.css -o src/styles/tailwind-output.css",
		"dev": "concurrently \"yarn run dev:only\" \"yarn run tailwind:watch\"",
		"build": "yarn run tailwind:build && yarn run build:only",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"gen": "graphql-codegen --config codegen.yml"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^3.3.0",
		"@graphql-codegen/introspection": "^3.0.1",
		"@graphql-codegen/named-operations-object": "^2.3.1",
		"@graphql-codegen/typescript": "^3.0.3",
		"@graphql-codegen/typescript-operations": "^3.0.3",
		"@graphql-codegen/typescript-urql": "^3.7.3",
		"@graphql-codegen/urql-introspection": "^2.2.1",
		"@graphql-codegen/urql-svelte-operations-store": "^1.3.3",
		"@sveltejs/adapter-auto": "^1.0.0-next.90",
		"@sveltejs/kit": "^1.0.0-next.587",
		"@types/uuid": "^9.0.1",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.14",
		"concurrently": "^8.0.1",
		"cross-env": "^7.0.3",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"graphql": "^16.6.0",
		"postcss": "^8.4.23",
		"postcss-cli": "^10.1.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.58.0",
		"svelte-check": "^2.9.2",
		"tailwindcss": "^3.3.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"dependencies": {
		"@popperjs/core": "^2.11.7",
		"@urql/svelte": "^4.0.0",
		"classnames": "^2.3.2",
		"date-fns": "^2.29.3",
		"flowbite": "^1.6.5",
		"flowbite-svelte": "^0.34.9",
		"svelte-frappe-charts": "^1.10.0",
		"urql": "^4.0.0",
		"uuid": "^9.0.0"
	},
	"type": "module"
}
